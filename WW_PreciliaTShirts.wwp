     code : |1+
      
      CONSTANT
      	SITE_NOM = "PRECILIA TSHIRTS"
      	
      	SITE_ADRESSE_POSTALE = [
      		3 rue de Puech Villa
      		34000 MONTPELLIER
      		FRANCE
      	]
      	SITE_GEOPOS_LATITUDE = 43.6396448
      	SITE_GEOPOS_LONGITUDE = 3.8372963
      	
      	SITE_TELEPHONE = "+33 (0)4.67.032.032"
      	SITE_EMAIL_CONTACT = "info@precilia.com"
      
      	// Constante pour les caractéristiques
      	Caractéristique_Taille = 0
      	Caractéristique_Couleur = 1
      	
      FIN
      
      // Déclaration d'un contexte AWP
      ConfigureContexteAWP(ctxDisque, ctxIDCookieURL)
      
      // Création/Ouverture de la base de données
      HCréationSiInexistant("*")
      
      // Initialisation du composant interne de gestion de ECommerce
      COL_ECommerce.InitialiseECommerce()
      
      // Initialisation du composant interne de gestion de l'espace client
      COL_ComptesClient.InitialiseGestionClients()
      
      // Dans ce projet on gère 2 décimales sur les prix
      COL_ECommerce.gnNbDécimales = 2
      // Initialisation des pages
      pPagesVenteEnLigne.InitialisationPage()
      // Déclaration des pages du projets appelées depuis la partie "site de vente"
      COL_ECommerce.gsURLPageVisuProduit = PageAdresse(Produit)
      COL_ECommerce.gsURLRetourBoutique = PageAdresse(boutique)
      COL_ECommerce.gsURLCGV = PageAdresse(conditions_generales_de_vente)
      
      LOCAL
      soit (bRésultat, sNomPagesErreurs) = COL_ECommerce.VérifiePagesSiteVente()
      dbgAssertion(bRésultat,"URL de pages non renseignées : "+sNomPagesErreurs)
