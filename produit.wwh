   -
     name : CocheVisible
     procedure_id : 1603416570118374120
     type_code : 14
     code : |1-
      // Résumé : Définit la coche à utiliser selon une couleur
      // Syntaxe :
      //[ <Résultat> = ] CocheVisible (<nCouleurOrigine> est entier)
      //
      // Paramètres :
      //	nCouleurOrigine (entier) :Couleur d'origine sur laquelle la coche doit être visible
      // Valeur de retour :
      // 	chaîne ANSI : Image de la coche à utiliser
      //
      PROCEDURE CocheVisible(LOCAL nCouleurOrigine est un entier)
      
      sImageCoche est une chaine
      rLuminance est un réel
      CouleurOrigine est une couleur = nCouleurOrigine
      
      
      // Calcul de la luminance de la couleur passée en paramètre
      rLuminance = 1 - (0.299 * CouleurOrigine..Rouge + 0.587 * CouleurOrigine..Vert + 0.114 * CouleurOrigine..Bleu)/255
      
      // En fonction de la luminance, la couleur d'écriture doit avoir une faible ou une forte luminosité
      SI rLuminance < 0.5 ALORS
      	// Faible luminosité
      	sImageCoche = "./images/coche_noir.png"
      SINON
      	// Forte luminosité
      	sImageCoche = "./images/coche_blanc.png"
      
      FIN
      
      renvoyer sImageCoche
