     name : AfficheTailleSelection
     procedure_id : 1614266851377016846
     type_code : 14
     code : |1+
      // Résumé : Positionne la coche sur la couleur sélectionnée
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE AfficheTailleSelection()
      
      // Recherche la taille de la déclinaison
      sTailleDeclinaison est une chaine
      pclDeclinaisonEnCours	est un CDeclinaisonProduit dynamique
      
      // On se positionne sur la déclinaison en cours
      pclDeclinaisonEnCours = gclProduitEnCours.pclDeclinaisonParId(gnIDDéclinaisonCourante)
      dbgAssertion(pclDeclinaisonEnCours<>Null,"Déclinaison non trouvée")
      
      // Recherche la taille de la déclinaison
      sTailleDeclinaison = pclDeclinaisonEnCours.GetValeurCaracteristiqueSelonNom("Taille") 
      
      // On positionne la liste
      ListeSelectPlus(COMBO_TAILLE,ListeCherche(COMBO_TAILLE,sTailleDeclinaison))
