     procedure_id : 1603416570118374120
     type_code : 14
     code : |1-
      // R√©sum√© : D√©finit la coche √† utiliser selon une couleur
      // Syntaxe :
      //[ <R√©sultat> = ] CocheVisible (<nCouleurOrigine> est entier)
      //
      // Param√®tres :
      //	nCouleurOrigine (entier) :Couleur d'origine sur laquelle la coche doit √™tre visible
      // Valeur de retour :
      // 	cha√Æne ANSI : Image de la coche √† utiliser
      //
      PROCEDURE CocheVisible(LOCAL nCouleurOrigine est un entier)
      
      sImageCoche est une chaine
      rLuminance est un r√©el
      CouleurOrigine est une couleur = nCouleurOrigine
      
      
      // Calcul de la luminance de la couleur pass√©e en param√®tre
      rLuminance = 1 - (0.299 * CouleurOrigine..Rouge + 0.587 * CouleurOrigine..Vert + 0.114 * CouleurOrigine..Bleu)/255
      
      // En fonction de la luminance, la couleur d'√©criture doit avoir une faible ou une forte luminosit√©
      SI rLuminance < 0.5 ALORS
      	// Faible luminosit√©
      	sImageCoche = "./images/coche_noir.png"
      SINON
      	// Forte luminosit√©
      	sImageCoche = "./images/coche_blanc.png"
      
      FIN
      
      renvoyer sImageCoche
