     name : AfficheProduit
     procedure_id : 1603390903327413423
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // AfficheProduit ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      
      PROCEDURE AfficheProduit()
      
      // Recherche la déclinaison 
      HLitRecherchePremier(DéclinaisonProduit, IDDéclinaisonProduit, gnIDDéclinaisonCourante)
      
      // Recherche le produit
      HLitRecherchePremier(produit, idproduit, DéclinaisonProduit.IDProduit)
      
      // Mémorise cette déclinaison comme déclinaison courante
      gnIDDéclinaisonCourante = DéclinaisonProduit.IDDéclinaisonProduit
      
      // Recherche la photo produit
      HLitRecherchePremier(PhotoProduit, IDDéclinaisonProduit, DéclinaisonProduit.IDDéclinaisonProduit)
      
      // Affiche les informations du produit
      FichierVersEcran()
      
      // Affiche / cache le libellé "Nouveauté"
      LIB_Nouveauté..Visible = produit.Nouveau
      
      // Remplit la ZR des photos de déclinaisons
      RemplitPhotosDéclinaisons()
      
      // Remplit la ZR de choix de couleur
      RemplitCouleursDéclinaisons()
     type : 458752
   -
     name : RemplitPhotosDéclinaisons
     procedure_id : 1603391027881558996
     type_code : 14
     code : |1-
      // Résumé : Remplit les photos de déclinaison du produit
      // Syntaxe :
      // RemplitPhotosDéclinaisons ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE RemplitPhotosDéclinaisons()
      
      // Vide la ZR
      ZoneRépétéeSupprimeTout(ZR_Photos)
      
      // Parcours les déclinaisons du produit
      pour TOUT DéclinaisonProduit avec IDProduit = produit.idproduit
      	
      	// Recherche la photo associée à la déclinaison
      	hlitrecherchepremier(PhotoProduit, IDDéclinaisonProduit, DéclinaisonProduit.IDDéclinaisonProduit)
      	si htrouve(PhotoProduit) ALORS
      		
      		// Affiche l'image (si elle n'est pas déjà présente)
      		si zonerepeteecherche(ATT_Photo,PhotoProduit.Photo) <= 0 alors 
      			ZoneRépétéeAjouteLigne(ZR_Photos, DéclinaisonProduit.IDDéclinaisonProduit, photoproduit.IDPhotoProduit, PhotoProduit.Photo)
      		Fin
      		
      	FIN
      	
      FIN
     type : 458752
   -
     name : RemplitCouleursDéclinaisons
     procedure_id : 1603405669485168778
     type_code : 14
     code : |1-
      // Résumé : Remplit les couleurs de déclinaison du produit
      // Syntaxe :
      //RemplitCouleursDéclinaisons ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE RemplitCouleursDéclinaisons()
      
      // Vide la ZR
      ZoneRépétéeSupprimeTout(ZR_Couleurs)
      
      // Parcours les déclinaisons du produit
      pour TOUT DéclinaisonProduit avec IDProduit = produit.idproduit
      	
      	// Recherche la couleur associée à la déclinaison
      	HLitRecherchePremier(DéclinaisonProduit_ValeurCaractéristique,IDDéclinaisonProduit,DéclinaisonProduit.IDDéclinaisonProduit)
      	HLitRecherchePremier(ValeurCaractéristique,IDValeurCaractéristique,DéclinaisonProduit_ValeurCaractéristique.IDValeurCaractéristique)
      	
      	// Affiche la couleur (si elle n'est pas déjà présente)
      	si zonerepeteecherche(ATT_Couleur,HTMLVersRVB("#"+ValeurCaractéristique.Valeur)) <= 0 alors
      		ZoneRépétéeAjouteLigne(ZR_Couleurs, DéclinaisonProduit.IDDéclinaisonProduit, HTMLVersRVB("#"+ValeurCaractéristique.Valeur), ValeurCaractéristique.ValeurAffichage, "./images/coche_vide.png","", ValeurCaractéristique.IDValeurCaractéristique)
      	Fin
      FIN
